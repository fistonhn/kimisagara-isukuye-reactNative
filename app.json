{
  "expo": {
    "name": "Kimisagara Isukuye",
    "slug": "Umurenge-wa-Kimisagara",
    "description": "DUHARANIRE KIMISAGARA IKEYE KANDI IZIRA IGWINGIRA",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/kimisagara-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/kimisagara-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/kimisagara-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.kimisagara.isukuye",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "710dee7c-6b06-403f-b5e7-e1b49884962e"
      }
    }
  }
}
